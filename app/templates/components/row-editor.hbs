<tr>
  <th>
    #
  </th>
  {{#each table.rows as |row|}}
    <th>{{row.name}} {{bs-button onClick=(action "deleteRow" row.id) type="alert" icon="glyphicon glyphicon-trash"}}</th>
  {{/each}}
  <th>
    {{#bs-button type="primary" onClick=(action "addRow")}}
      Add field
    {{/bs-button}}
  </th>
</tr>
